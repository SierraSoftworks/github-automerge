{
  "name": "github-automerge",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc && npm run sentry:sourcemaps",
    "watch": "tsc -w",
    "prestart": "npm run build",
    "start": "func start",
