    "@sentry/cli": "^2.21.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^27.5.0",
    "@types/node": "^20.8.9",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@azure/functions": "^4.0.1",
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.4",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.44.0",
    "@opentelemetry/sdk-node": "^0.44.0",
